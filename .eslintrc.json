{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true,
    "jest": true
  },
  "extends": ["plugin:vue/recommended", "airbnb-base"],
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "globals": {
    "velocity": true
  },
  "rules": {
    "no-console": 0,
    "no-alert": 0,
    "no-debugger": 1,
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "bracketSpacing": true,
        "arrowParens": "always"
      }
    ]
  }
}
